{
  "env": {
    "browser": true,
    "es2021": true
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "no-const-assign": 0
  }
}
